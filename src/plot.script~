set terminal x11 0

set title "Insertion Sort Algorithms"
set xlabel "Time"
set ylabel "Size"

plot "insertion_sort_i.dat" title "" with linespoints ls 1
replot "insertion_sort_r.dat" title "" with linespoints ls 2
replot "insertion_sort_b.dat" title "" with linespoints ls 3
replot "counting_inversions.dat" title "" with linespoints ls 4

set terminal x11 1

set title "Quick Sort Algorithms"
set xlabel "Time"
set ylabel "Size"

plot "quick_sort_l.dat" title "" with linespoints ls 1
replot "quick_sort_f.dat" title "" with linespoints ls 2
replot "quick_sort_r.dat" title "" with linespoints ls 3
replot "quick_sort_m.dat" title "" with linespoints ls 4

set terminal x11 2


set title "Hybrid Sort Algorithms"
set xlabel "Time"
set ylabel "Size"

plot "merge_insertion_sort.dat" title "" with linespoints ls 1
replot "quick_insertion_sort" title "" with linespoints ls 2

set terminal x11 3

set title "Search Algorithms"
set xlabel "Time"
set ylabel "Size"

plot "merge_insertion_sort.dat" title "" with linespoints ls 1
replot "quick_insertion_sort" title "" with linespoints ls 2

